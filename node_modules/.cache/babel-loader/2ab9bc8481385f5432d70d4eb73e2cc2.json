,SAAA,GAAA,IAAA,CAAA,KAAA,CAAA,OAAA,CAAA,GAAA,CAAA,OAAA,CAAA,CAAA,CAJA,CAKA;;AACA,MAAA,YAAA,CAAA,OAAA,CAAA,WAAA,EAAA,IAAA,CAAA,SAAA,CAAA,SAAA,CAAA,EAAA,CAAA;AACA;AAEA,WAAA,cAAA,GAAA,MAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,EAAA;AACA,cAAA,QAAA,GAAA,EAAA;AACA,UAAA,QAAA,GAAA,KAAA,CAAA,QAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA;AACA,gBAAA,CAAA,MAAA,CAAA,WAAA,CAAA,KAAA,CAAA,EAAA;AACA;AACA;;AAEA,gBAAA,CAAA,MAAA,CAAA,aAAA,CAAA,UAAA,EAAA,KAAA,CAAA,EAAA;AACA;AACA;;AAEA,mBAAA,KAAA;AACA,WAVA,CAAA;AAWA,UAAA,KAAA,CAAA,QAAA,GAAA,QAAA;AACA;;AAEA,YAAA,CAAA,MAAA,CAAA,WAAA,CAAA,KAAA,CAAA,EAAA;AACA;AACA;;AAEA,YAAA,CAAA,MAAA,CAAA,aAAA,CAAA,UAAA,EAAA,KAAA,CAAA,EAAA;AACA;AACA;;AAEA,eAAA,KAAA;AACA,OA1BA,CAAA;AA2BA;AArCA,GA9BA;AAqEA,EAAA,KAAA,EAAA;AACA,IAAA,KADA,iBACA,MADA,EACA,MADA,EACA;AAEA,UAAA,MAAA,IAAA,EAAA,EAAA;AACA,aAAA,cAAA,GAAA,EAAA;AACA,OAFA,MAEA;AACA,aAAA,aAAA;AACA;AACA;AARA;AArEA,CAAA","sourcesContent":["<template>\r\n    <div class=\"side-bar-wrapper\" :class=\"{'collapsed-sidebar': isCollapse}\">\r\n        <el-menu\r\n            :default-active=\"activeMenu\"\r\n            :collapse=\"isCollapse\"\r\n            :background-color=\"variables.menuBg\"\r\n            :text-color=\"variables.menuText\"\r\n            :unique-opened=\"false\"\r\n            :active-text-color=\"variables.menuActiveText\"\r\n            :collapse-transition=\"false\"\r\n            mode=\"vertical\"\r\n        >\r\n            <div class=\"menu-wrapper\" v-for=\"(firstRoute, firstIndex) in accessedRoutes\" :key=\"firstIndex\">\r\n                <el-submenu v-if=\"firstRoute.children && firstRoute.children.length > 1\" :index=\"firstRoute.path\">\r\n                    <template slot=\"title\">\r\n                        <i :class=\"firstRoute.meta.icon\"></i>\r\n                        <span slot=\"title\">{{firstRoute.meta.title}}</span>\r\n                    </template>\r\n                    <router-link :to=\"firstRoute.path + '/' + secondRoute.path\" :key=\"firstIndex + '-' + secondIndex\"  v-for=\"(secondRoute, secondIndex) in firstRoute.children\">\r\n                        <el-menu-item :index=\"firstRoute.path + '/' + secondRoute.path\">{{secondRoute.meta.title}}</el-menu-item>\r\n                    </router-link>\r\n                </el-submenu>\r\n\r\n                <router-link v-else :to=\"firstRoute.path + '/index'\">\r\n                    <el-menu-item :index=\"firstRoute.path + '/index'\" >\r\n                        <i :class=\"firstRoute.meta.icon\"></i>\r\n                        <span slot=\"title\">{{firstRoute.meta.title}}</span>\r\n                    </el-menu-item>\r\n                </router-link>\r\n            </div>\r\n        </el-menu>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport variables from '@/styles/variables.scss'\r\nimport { routes } from '@/router'\r\nimport cookies from 'js-cookie'\r\nimport CONSTANTS from '@/utils/constants'\r\nimport common from '@/utils/common'\r\n\r\nexport default {\r\n    computed: {\r\n        ...mapGetters([\r\n            'sidebar',\r\n            'token'\r\n        ]),\r\n        isCollapse() {\r\n            return !this.sidebar.opened\r\n        },\r\n        variables() {\r\n            return variables\r\n        },\r\n        activeMenu() {\r\n            const route = this.$route\r\n            const { meta, path } = route\r\n            // if set path, the sidebar will highlight the path you set\r\n            if (meta.activeMenu) {\r\n                return meta.activeMenu\r\n            }\r\n            return path\r\n        },\r\n    },\r\n    data() {\r\n        return {\r\n            accessedRoutes: []\r\n        }\r\n    },\r\n    created() {\r\n        this.generateRoute()\r\n    },\r\n    methods: {\r\n        generateRoute() {\r\n            const adminType = JSON.parse(cookies.get('admin')).type\r\n            const adminTitle = CONSTANTS.ADMIN_TYPE[adminType]\r\n\r\n            const adminList = JSON.parse(cookies.get('admin'))\r\n            // console.log(adminList)\r\n            localStorage.setItem(\"adminList\", JSON.stringify(adminList.id));\r\n            debugger\r\n\r\n            this.accessedRoutes = routes.filter((route) => {\r\n                if (route.children) {\r\n                    let children = []\r\n                    children = route.children.filter ((child) => {\r\n                        if (!common.isRouteShow(child)) {\r\n                            return\r\n                        }\r\n\r\n                        if (!common.isRouteAccess(adminTitle, child)) {\r\n                            return\r\n                        }\r\n\r\n                        return child\r\n                    })\r\n                    route.children = children\r\n                }\r\n\r\n                if (!common.isRouteShow(route)) {\r\n                    return\r\n                }\r\n\r\n                if (!common.isRouteAccess(adminTitle, route)) {\r\n                    return\r\n                }\r\n\r\n                return route\r\n            }) \r\n        }\r\n    },\r\n    watch: {\r\n        token(newVal, oldVal) {\r\n\r\n            if (newVal == '') {\r\n                this.accessedRoutes = []\r\n            } else {\r\n                this.generateRoute()\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import \"~@/styles/variables.scss\";\r\n.side-bar-wrapper {\r\n    position: fixed;\r\n    top: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    width: $sideBarWidth;\r\n    height: 100%;\r\n    background-color: $menuBg;\r\n    z-index: 1001;\r\n    overflow: hidden;\r\n    transition: width 0.28s;\r\n}\r\n\r\n.side-bar-wrapper.collapsed-sidebar {\r\n    width: $hideSideBarWidth;\r\n}\r\n\r\n.el-menu--collapse {\r\n    .el-submenu__title {\r\n        .el-submenu__icon-arrow {\r\n            display: none;\r\n\r\n        }\r\n        span {\r\n            height: 0;\r\n            width: 0;\r\n            overflow: hidden;\r\n            visibility: hidden;\r\n            display: inline-block;\r\n        }\r\n    }\r\n}\r\n\r\n\r\n.el-submenu__title {\r\n    text-align: left;\r\n}\r\n.el-submenu {\r\n    .el-menu-item {\r\n        text-align: left;\r\n        background-color: #1f2d3d !important;\r\n        &:hover {\r\n            background-color: #001528 !important;\r\n        }\r\n    }\r\n}\r\n.el-menu-item {\r\n    text-align: left;\r\n}\r\n.el-menu {\r\n    border: none;\r\n    a {\r\n        text-decoration: none;\r\n    }\r\n}\r\n</style>"],"sourceRoot":"src/layout/components/SideBar"}]}